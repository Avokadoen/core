GridLayout {
	columns: expand, auto;
	rows: expand, auto;
	margins: 0;
	Panel {
		margins: 0;
		StackLayout {
			orientation: vertical;
			Slider {
				orientation: vertical;
			}
			Slider {
				orientation: horizontal;
			}
		}
	}
	ScrollBar {
		orientation: vertical;
		dock-site: right;
		margins: 0;
	}
	ScrollBar {
		orientation: horizontal;
		dock-site: bottom;
		margins: 0;
	}
	Spacer { }
}
